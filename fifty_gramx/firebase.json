                        "android": "1:1048555807763:android:f30ab92102bfcb3740cbe6",
                        "ios": "1:1048555807763:ios:c49421f1468b3dc340cbe6",
                        "macos": "1:1048555807763:ios:701fcc12bef6c05840cbe6",
                        "web": "1:1048555807763:web:5c1e0a6f9cd7b38e40cbe6",
